{
  "name": "ngx-ui-scroll-app",
  "version": "0.0.0",
  "description": "Angular Demo App for ngx-ui-scroll",
  "main": "./index.html",
  "scripts": {
    "start": "npm-run-all --parallel start-api-server start-dev-server",
    "start-dev-server": "ng serve --proxy-config proxy.conf.json",
    "start-api-server": "node ./server/index.js",
    "start-env": "node env",
    "lint": "tslint -c tslint.json  --project . ",
    "test": "npm run lint && karma start",
    "build-app": "ng build --prod --aot --source-map",
    "deploy-app": "npm run build-app && firebase deploy",
    "preinstall": "cd server && npm install",
    "postinstall": "npm run build-app",
    "pack:install": "npm run build && npm pack ./dist && npm install ngx-ui-scroll-2.0.0-beta.tgz --no-save",
    "pack:start": "npm run pack:install && npm start",
    "build": "node build.js",
    "publish:lib": "npm run build && npm publish ./dist",
    "publish:lib:next": "npm run build && npm publish --tag next ./dist"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "11.0.7",
    "@angular/common": "11.0.7",
    "@angular/core": "11.0.7",
    "@angular/forms": "11.0.7",
    "@angular/platform-browser": "11.0.7",
    "@angular/platform-browser-dynamic": "11.0.7",
    "@angular/router": "11.0.7",
    "ngx-bootstrap": "^5.6.1",
    "rxjs": "~6.6.1",
    "tslib": "^1.13.0",
    "vscroll": "^1.0.0-beta.4",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1100.6",
    "@angular/cli": "^11.0.6",
    "@angular/compiler": "11.0.7",
    "@angular/compiler-cli": "11.0.7",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@types/jasmine": "^3.5.0",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^14.0.1",
    "angular2-template-loader": "^0.6.2",
    "chalk": "^4.0.0",
    "codelyzer": "^6.0.1",
    "core-js": "~3.6.5",
    "css-loader": "^5.0.1",
    "dotenv": "^8.2.0",
    "jasmine-core": "~3.5.0",
    "karma": "^5.0.5",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "~2.0.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^3.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^4.0.2",
    "npm-run-all": "^4.1.3",
    "raw-loader": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rollup": "^2.10.2",
    "rollup-plugin-license": "^2.2.0",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "sass-loader": "^8.0.2",
    "shelljs": "^0.8.4",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^7.0.4",
    "tslint": "^6.1.0",
    "tslint-angular": "^3.0.2",
    "typescript": "4.0.5",
    "uglify-js": "^3.9.3",
    "webpack": "^4.45.0",
    "webpack-filter-warnings-plugin": "^1.2.1"
  }
}
